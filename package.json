{
  "name": "hangman-api",
  "version": "0.0.1",
  "description": "API for Hangman Game",
  "author": "Dante Calderon<mail@dantecalderon.dev>",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "node dist/main",
    "prestart:dev": "docker-compose up -d",
    "start:dev": "nodemon",
    "start:debug": "nodemon --config nodemon-debug.json",
    "prestart:prod": "rimraf dist && npm run build",
    "start:prod": "node dist/main.js",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:all": "npm run test && npm run test:e2e",
    "test:watch": "jest --watch",
    "test:cov": "jest --config ./test/jest-coverage.json --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@dantehemerson/nestjs-dataloader": "1.0.1-alpha.0",
    "@nestjs-modules/mailer": "^1.6.1",
    "@nestjs/apollo": "^10.0.12",
    "@nestjs/common": "^8.4.5",
    "@nestjs/config": "^2.0.1",
    "@nestjs/core": "^8.4.5",
    "@nestjs/graphql": "^10.0.12",
    "@nestjs/jwt": "^8.0.1",
    "@nestjs/passport": "^8.2.1",
    "@nestjs/platform-fastify": "^8.4.5",
    "@nestjs/swagger": "^5.2.1",
    "@ntegral/nestjs-sentry": "^1.0.6",
    "@typegoose/typegoose": "^9.8.1",
    "@types/bcrypt": "^3.0.0",
    "apollo-datasource-rest": "^3.6.0",
    "apollo-server-core": "^3.8.1",
    "apollo-server-fastify": "^3.8.1",
    "automapper-nartc": "^6.1.3",
    "aws-sdk": "^2.1141.0",
    "bcrypt": "^5.0.1",
    "bluebird": "^3.7.2",
    "bunyan": "^1.8.15",
    "bunyan-prettystream": "^0.1.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "faker": "^6.6.6",
    "fastify-swagger": "^5.2.0",
    "gitter-markdown-processor": "^18.0.0",
    "graphql": "^15.8.0",
    "graphql-subscriptions": "^2.0.0",
    "graphql-tools": "^8.2.11",
    "helmet": "^3.22.0",
    "lodash.get": "^4.4.2",
    "lodash.keyby": "^4.6.0",
    "mongodb-memory-server": "^6.10.0",
    "mongoose": "^6.3.4",
    "nestjs-typegoose": "^7.1.38",
    "nodemailer": "^6.4.10",
    "passport": "^0.4.0",
    "passport-google-oauth": "^2.0.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.1.0",
    "slugify": "^1.4.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.2.6",
    "@nestjs/testing": "^8.4.5",
    "@sentry/types": "^6.19.7",
    "@types/faker": "^6.6.9",
    "@types/jest": "27.5.1",
    "@types/lodash.get": "^4.4.7",
    "@types/lodash.keyby": "^4.6.7",
    "@types/mongoose": "^5.11.97",
    "@types/node": "^14.18.18",
    "@types/nodemailer": "^6.4.0",
    "@types/passport-google-oauth": "^1.0.41",
    "@types/passport-jwt": "^3.0.3",
    "@types/passport-local": "^1.0.33",
    "@types/supertest": "2.0.8",
    "concurrently": "^5.2.0",
    "jest": "^25.4.0",
    "nodemon": "^2.0.3",
    "prettier": "^2.0.5",
    "supertest": "^4.0.2",
    "ts-jest": "25.4.0",
    "ts-node": "10.8.0",
    "tsconfig-paths": "4.0.0",
    "tslint": "6.1.2",
    "typescript": "4.7.2",
    "wait-on": "6.0.1"
  },
  "resolutions": {
    "graphql": "15.8.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "engines": {
    "node": "12.x"
  }
}
